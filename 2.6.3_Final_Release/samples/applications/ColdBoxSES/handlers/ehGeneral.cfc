<!-----------------------------------------------------------------------Author 	 :	Your NameDate     :	September 25, 2005Description : 				This is a ColdBox event handler for general methods.Please note that the extends needs to point to the eventhandler.cfcin the ColdBox system directory.extends = coldbox.system.eventhandler	-----------------------------------------------------------------------><cfcomponent name="ehGeneral" extends="coldbox.system.eventhandler" output="false">		<cfset this.PREHANDLER_ONLY = "dspPrintSample">		<!--- preHandler --->
	<cffunction name="preHandler" access="public" returntype="void" output="false" hint="">
		<cfargument name="Event" type="coldbox.system.beans.requestContext" required="yes">
	    <cfset var rc = event.getCollection()>
	    <cfif len(Event.getValue('printit',''))>			<cfset Event.setLayout('Layout.Print') />		</cfif>
	</cffunction>		<!--- This init is mandatory, including the super.init(). ---> 	<cffunction name="init" access="public" returntype="ehGeneral" output="false">		<cfargument name="controller" type="any">		<cfset super.init(arguments.controller)>		<!--- Any constructor code here --->		<cfreturn this>	</cffunction>	<cffunction name="index" access="public" returntype="void" output="false">		<cfargument name="Event" type="any">		<!--- Do Your Logic Here to prepare a view --->		<cfset Event.setValue("welcomeMessage","Welcome to ColdBox!")>			<!--- Set the View To Display, after Logic --->		<cfset Event.setView("ehGeneral/index")>	</cffunction>		<cffunction name="dspPrintSample" access="public" returntype="void" output="false">		<cfargument name="Event" type="any">		<!--- Do Your Logic Here, call to models, etc.--->		<cfset Event.setValue("welcomeMessage","Welcome to ColdBox!")>				<cfset Event.setView("printable_sample")>	</cffunction>	</cfcomponent>