<!-----------------------------------------------------------------------Author 	 :	Your NameDate     :	September 25, 2005Description : 				This is a ColdBox event handler for general methods.Please note that the extends needs to point to the eventhandler.cfcin the ColdBox system directory.extends = coldbox.system.eventhandler	-----------------------------------------------------------------------><cfcomponent name="ehSES" extends="coldbox.system.eventhandler" output="false">		<!--- This init is mandatory, including the super.init(). ---> 	<cffunction name="init" access="public" returntype="ehSES" output="false">		<cfargument name="controller" type="any">		<cfset super.init(arguments.controller)>		<!--- Any constructor code here --->		<cfreturn this>	</cffunction>	<cffunction name="index" access="public" returntype="void" output="false">		<cfargument name="Event" type="any">		<cfset Event.setValue("welcomeMessage","Welcome to ColdBox!")>					<cfset Event.setView("ehSES/index")>	</cffunction>		<cffunction name="testcase1" access="public" returntype="void" output="false">		<cfargument name="Event" type="any">		<cfset var rc = Event.getCollection() />		<cfset Event.setValue("welcomeMessage","Welcome to SES test case1!")>		<cfset Event.setView("ehSES/testcase1")>	</cffunction>		<cffunction name="testcase2" access="public" returntype="void" output="false">		<cfargument name="Event" type="any">		<cfset var rc = Event.getCollection() />		<cfset Event.setValue("welcomeMessage","Welcome to SES test case2!")>		<cfset Event.setView("ehSES/testcase2")>	</cffunction></cfcomponent>